   "source": [
    "import numpy as np\n",
    "\n",
    "# Given matrix\n",
    "A = np.array([[1, 0.2, 0.5],\n",
    "              [0.2, 1, 0.8],\n",
    "              [0.5, 0.8, 1]])\n",
    "\n",
    "# Calculate the inverse of the matrix\n",
    "A_inv = np.linalg.inv(A)\n",
    "\n",
    "print(\"Original Matrix A:\")\n",
    "print(A)\n",
    "print(\"\\nInverse of Matrix A:\")\n",
    "print(A_inv)\n"
   ]
  }
